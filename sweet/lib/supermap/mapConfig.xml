    <!--地图比例尺刻度切换放大倍数-->
    <leaveStep>1</leaveStep>
    
    <!--地图缩放级别设置方案一、二共用参数 -->
    <!--地图比例尺总刻度数-->
    <scalesLeave>18</scalesLeave>
